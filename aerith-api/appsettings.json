{
  "ConnectionStrings": {
    "AerithDB": "{{hidden}}",
    "IdentityDataContextConnection": "Server=(localdb)\\mssqllocaldb;Database=aerith-api;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "aerith-vue": {
        "Profile": "SPA",
        "RedirectUri": "http://localhost:8080/authentication/login-callback",
        "LogoutUri": "http://localhost:8080/authentication/logout-callback"
      }
    }
  },
  "AllowedHosts": "*"
}